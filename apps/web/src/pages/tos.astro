---
import MarketingLayout from '../layouts/MarketingLayout.astro'
import SEO from '../components/SEO.astro'
import { getPageTitle, brand } from '@repo/config/brand'
---

<MarketingLayout>
    <Fragment slot="head">
        <SEO
            title={getPageTitle('Terms of Service')}
            description={`Read our terms of service to understand the rules and regulations for using ${brand.name}.`}
        />
    </Fragment>

    <div class="container mx-auto py-16">
        <h1 class="text-4xl font-bold mb-8">Terms of Service</h1>
        <div>Terms of service content</div>
    </div>
</MarketingLayout>
